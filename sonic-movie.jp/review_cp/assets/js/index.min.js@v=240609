"use strict";$(function(){function n(){new PerfectScrollbar("#js-ps-terms")}function s(){var e=$(".terms__cont"),t=window.innerHeight-140;$(window).width()<=768?e.height(t+"px"):e.removeAttr("style")}var t=$("#js-adjustPresentTextHeight .present__item").length;function i(){$(".present__name").removeAttr("style"),$(".present__winners").removeAttr("style");var n,e=$(window).width();n=e<=768?2:4,Math.floor(t/n);$("#js-adjustPresentTextHeight .present__item").each(function(e,t){e=Math.floor((e+n)/n);$(t).attr("data-rows",e),$(t).find(".present__name"),$(t).find(".present__winners");$('[data-rows="'+e+'"] .present__name').matchHeight(),$('[data-rows="'+e+'"] .present__winners').matchHeight()})}$(".js-inc").each(function(){var t=$(this).data("inc"),e=$(this).data("version")||0;$(this).load("./assets/include/_"+t+".html?v="+e,function(){var e;"schedule"==t&&$("#js-scheduleSwitch time").length&&(e=new Date,new Date($("#js-scheduleSwitch time").attr("datetime"))<e)&&$("#js-scheduleSwitch").addClass("is-end"),"howto"==t&&$("img.js-replaceSVG").each(function(e,t){var n=$(t).data("src");$.get(n,function(e){e=(new XMLSerializer).serializeToString(e.documentElement);$(t).replaceWith(e)},"xml")}),"present"==t&&i(),"terms"==t&&($(".js-terms-close").on("click",function(){$(".terms").fadeOut(),$("html, body").css("overflow","visible")}),$(".js-terms-open").on("click",function(){$(".terms").fadeIn(),$("html, body").css("overflow","hidden"),n()}),n(),s())})}),$(window).on("load",function(){$("html").addClass("is-loaded"),setTimeout(function(){$('a[href*="#"]').on("click",function(){var e=$(this).attr("href"),t=e.indexOf("#"),e=e.substr(t),t=$(e).offset().top;return $("html, body").animate({scrollTop:t},800,"easeOutCubic"),!1})},500)}),$(window).on("load scroll resize",function(){s()}),$(window).on("load resize",function(){i()}),$(document).on("contextmenu",function(e){e.preventDefault()})});