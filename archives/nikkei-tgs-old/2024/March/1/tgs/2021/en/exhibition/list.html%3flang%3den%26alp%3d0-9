      <script>
        $(document).ready(function(){
        const TGSobj = window.TGS_expo;
        
        TGSobj.init();
        
        function getParam(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        
        return decodeURIComponent(results[2].replace(/\+/g, " "));
        }

        function sanitize_decode (str) {
          return String(str)
            .replace(/&amp;/g, '&')
            .replace(/&quot;/g, '"')
            .replace(/&#039;/g, '\'')
            .replace(/&#x60;/g, '`')
            .replace(/&lt;/g, '<')
            .replace(/&gt;/g, '>')
        }
        
        function sanitize(str) {
          return sanitize_decode(String(str))
            .replace(/&/g, '&amp;')
            .replace(/"/g, '&quot;')
            .replace(/'/g, '&#039;')
            .replace(/`/g, '&#x60;')
            .replace(/</g, '&lt;')
            .replace(/>/g, '&gt;');
        }
        
        if(getParam('cat')){
        var lang = getParam('lang');
        var cat = getParam('cat');
        var args = {
        'lang': lang,
        'cat': cat,
        };
        }
        
        if(getParam('asc')){
        var lang = getParam('lang');
        var asc = getParam('asc');
        var args = {
        'lang': lang,
        'asc': asc,
        };
        }
        
        if(getParam('alp')){
        var lang = getParam('lang');
        var alp = getParam('alp');
        var args = {
        'lang': lang,
        'alp': alp,
        };
        }
        
        if(getParam('grp')){
        var lang = getParam('lang');
        var grp = getParam('grp');
        var args = {
        'lang': lang,
        'grp': grp,
        };
        }
        
        var target = 'exhibit';
        var display_area = '#gameGenreListWrap .exhibitorList';
        
        TGSobj.exec_ajax(target, args, display_area, lang);
        
        });
      </script>
