"use strict";$(function(){function e(e,o){o=o||window.location.href,e=e.replace(/[\[\]]/g,"\\$&");var a=new RegExp("[?&]"+e+"(=([^&#]*)|&|#|$)").exec(o);return a?a[2]?decodeURIComponent(a[2].replace(/\+/g," ")):"":null}$(".nav__item").on("click",function(){var e=$(this).data("page");$(this).hasClass("is-current")||($(".nav__item.is-current").removeClass("is-current"),$(this).addClass("is-current"),$(".cp__page.is-current").removeClass("is-current"),$(".cp__page."+e).addClass("is-current"))}),$(".accordion__head").on("click",function(){$(this).next(".accordion__body").slideToggle(),$(this).parent(".accordion__item").toggleClass("is-active")}),$(".js-modal-open").on("click",function(e){var o=$(this).data("modal");e.preventDefault(),$("html, body").css("overflow","hidden"),$('.modal[data-modal="'+o+'"]').fadeIn()}),$(".modal__close, .modal__overlay").on("click",function(e){e.preventDefault(),$("html, body").css("overflow","visible"),$(".modal").fadeOut()}),console.log(e("option")),e("option")&&("caravan"===e("option")?$(".nav__item.-caravan").trigger("click"):"standy"===e("option")&&$(".nav__item.-standy").trigger("click")),$(window).on("load",function(){$("body").addClass("is-loaded"),$(".loader").fadeOut(300)})});