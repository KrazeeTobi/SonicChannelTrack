"use strict";$(function(){$(".js-modal-open").on("click",function(o){var d=$(this).data("modal");o.preventDefault(),$("html, body").css("overflow","hidden"),$('.modal[data-modal="'+d+'"]').fadeIn()}),$(".modal__close, .modal__overlay").on("click",function(o){o.preventDefault(),$("html, body").css("overflow","visible"),$(".modal").fadeOut()}),$(window).on("load",function(){$("body").addClass("is-loaded"),$(".loader").fadeOut(300)})});