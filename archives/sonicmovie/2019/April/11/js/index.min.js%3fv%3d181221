"use strict";var video=document.getElementById("video"),body=document.getElementsByTagName("body")[0];function addLoadedClass(){body.classList.add("is-loaded")}function toggleShare(){var e=document.getElementById("js-share-btn"),d=document.getElementById("js-share-list");e.addEventListener("click",function(){d.classList.toggle("is-active")},!1)}video.addEventListener("ended",function(){body.classList.add("is-movieEnd")},!1),document.addEventListener("DOMContentLoaded",function(){addLoadedClass(),toggleShare()},!1);