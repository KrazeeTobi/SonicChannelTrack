var arBannerASrc = new Array();
var arBannerAAlt = new Array();
var arBannerAHref = new Array();
var arBannerBSrc = new Array();
var arBannerBAlt = new Array();
var arBannerBHref = new Array();

arBannerASrc[0] = "NextGenVideos_animated.gif";
arBannerAAlt[0] = "NextGen Videos";
arBannerAHref[0] = "http://www.nextgenvideos.com/";

