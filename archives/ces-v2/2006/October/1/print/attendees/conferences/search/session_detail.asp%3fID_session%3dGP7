        var parentLinks=new Array('dummy','/print/attendees/default.htm','/print/attendees/conferences/default.asp','/print/attendees/conferences/search_by_name.asp');
    
        function checkActiveNav(theUrl) {
            var linkUrl=theUrl;
            var returnValue=false;
            
            if (linkUrl==thisPageUrl) {
                returnValue=true;
            } else if (parentLinks.length>2 && linkUrl==parentLinks[parentLinks.length-1]) {
