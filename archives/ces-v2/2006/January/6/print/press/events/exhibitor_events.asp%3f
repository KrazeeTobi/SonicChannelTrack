var parentLinks=new Array('dummy','/print/press/default.htm','/print/press/events/default.asp');

function checkActiveNav(theUrl) {
    var linkUrl=theUrl;
    var returnValue=false;
    
    if (linkUrl==thisPageUrl) {
        returnValue=true;
