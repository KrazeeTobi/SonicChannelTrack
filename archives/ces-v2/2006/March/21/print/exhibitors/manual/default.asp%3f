var parentLinks=new Array('dummy','/print/exhibitors/default.htm');

function checkActiveNav(theUrl) {
    var linkUrl=theUrl;
    var returnValue=false;
    
    if (linkUrl==thisPageUrl) {
        returnValue=true;
