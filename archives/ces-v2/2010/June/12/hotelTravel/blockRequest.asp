	function checkemail(myEmail){
	var str= myEmail
	var filter=/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i
	if (filter.test(str))
		return true;
	else
		{
		alert("Please enter a valid e-mail address.")
		return false;
		}	
	}
