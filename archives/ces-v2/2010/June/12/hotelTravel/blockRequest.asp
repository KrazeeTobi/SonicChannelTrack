		return false
		}		
			
		if((year=="2010")&&(month == "12")&&(day < "26")){
		alert("Your requested dates must be between 12/26/10 and 1/16/11.")
		passedForm.input7.focus
		return false
		}
		
		if((year=="2011")&&(month != "01")){
		alert("Your requested dates must be between 12/26/10 and 1/16/11.")
		passedForm.input7.focus
		return false
		}		
			
		if((year=="2011")&&(month == "01")&&(day > "16")){
		alert("Your requested dates must be between 12/26/10 and 1/16/11.")
		passedForm.input7.focus
		return false
		}
		}
		
		if(passedForm.input6.value.length == 10){
		var month, day, year
