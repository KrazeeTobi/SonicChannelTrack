<script language="javascript1.2">
function validForm(passedForm)
	{
	var stringFirstName = document.mp_form.FirstName.value;
	var stringLastName = document.mp_form.LastName.value;
	var stringCompany = document.mp_form.Company.value;
	var stringAddress = document.mp_form.Address.value;
	var stringCity = document.mp_form.City.value;
	var stringPhone = document.mp_form.Phone.value;
	
		if (stringFirstName.length == 0) {
			alert("Please enter a first name."); mp_form.FirstName.focus(); return false; } 
	
		if (stringLastName == false) {
			alert("Please enter a last name."); mp_form.LastName.focus(); return false; } 
	
		if (stringCompany == false) {
			alert("Please enter a company name."); mp_form.Company.focus(); return false; } 
	
		if (stringAddress == false) {
			alert("Please enter a street address."); mp_form.Address.focus(); return false; } 
	
		if (stringCity == false) {
			alert("Please enter a city."); mp_form.City.focus(); return false; } 
	
		if (stringPhone == false) {
			alert("Please enter a phone number."); mp_form.Phone.focus(); return false; } 
	
		if (checkemail(passedForm.Email.value)==false) {
			passedForm.Email.focus(); return false; }
	
	return true;
	}

function checkemail(myEmail)
{
	var str= myEmail
	var filter=/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i
	if (filter.test(str))
		return true;
	else
		{
		alert("Please enter a valid e-mail address.")
		return false;
		}	
}
</script>

 

