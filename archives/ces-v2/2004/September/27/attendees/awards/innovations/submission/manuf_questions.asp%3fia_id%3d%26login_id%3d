<script language="JavaScript">
function CheckFields(){

	if(document.frm_member.member.value == ""){
		alert("Please tell us if the manufacturer is a member of CEA.")
		return false
	}
	if(document.frm_member.isexhibitor.value == ""){
		alert("Please tell us if the manufacturer is a 2004 International CES Exhibitor.")
		return false
	}
	if(document.frm_member.diffManuf.value == ""){
		alert("Please tell us if the manufacturer is the same as the submitter.")
		return false
	}
}
</script>
