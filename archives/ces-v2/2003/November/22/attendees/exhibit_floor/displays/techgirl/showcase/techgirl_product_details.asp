				var arrWords = text.split(/\s+/);
				if(arrWords[0] == /\s+/ || arrWords[0] == "")
					return(0);
				else
					return(arrWords.length);
			}else{
				return("This functionality requires version 4.0 or above of\neither Netscape or Internet Explorer.");
			}
		}
