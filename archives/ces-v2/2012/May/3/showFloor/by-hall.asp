   $('#LVCCFloorMapImageArea').css('top', $('#LVCCFloorMapImage').position().top);
   $('#LVCCFloorMapImageArea').css('left', $('#LVCCFloorMapImage').position().left);
   $('#VenetianFloorMapImageArea').css('top', $('#VenetianFloorMapImage').position().top);
   $('#VenetianFloorMapImageArea').css('left', $('#VenetianFloorMapImage').position().left);  
 
   $('#CityViewImageArea').css('top', $('#CityViewImage').position().top);
   $('#CityViewImageArea').css('left', $('#CityViewImage').position().left);  
  }
  function showMapInfo(selectedObject) {
   $('.map-content').hide();
   $($(selectedObject).attr("selection")).show();
   buildGreenHeaderBox($('div.sponsorshipTitle:visible').html(), ($('div.booth:visible').html() ? $('div.booth:visible').html() :'' ));
  }
  function buildGreenHeaderBox(firstLine, secondLine) {
   $('#map-header').children().not('img').remove()
   $('<h2/>', {
    text: firstLine
   }).appendTo('#map-header');
   $('<h3/>', {
    text: secondLine
   }).appendTo('#map-header');
  }
  
        $(document).ready(function(){
   $('<link />', {
                    href: 'showFloorMap.css',
                    media: 'screen',
                    rel: 'stylesheet',
                    type: 'text/css',
                    'class': 'showFloorMap'
                }).appendTo('head');
            if ($.browser.msie) {
                $('.showFloorMap').clone().appendTo('head');
            }
 
   positionImageMap();
 
   $('.LVCCMapContent').show();
   $('.VenetianMapContent').hide();
   $('.CityViewContent').hide();
   buildGreenHeaderBox('Exhibit Map 2012', 'LVCC');
 
   $('#LVCCMap area, #VenetianMap area, #CityViewMap area').live('mouseout', function(event) {
    // move to the top of the sprite when the mouse isn't over a piece of the map. the top is the "base" image"
    $('#LVCCFloorMapImage').css('margin-top', '0px');
    $('#VenetianFloorMapImage').css('margin-top', '0px');
    $('#CityViewImage').css('margin-top', '0px');
   });
   
   $('#VenetianMap area, #LVCCMap area, #CityViewMap area').live('mousemove', function(event) {
    // this is a simple map of ID values of 'this' to the corresponding image that will be shifted
    var map = {"LVCCMap":"#LVCCFloorMapImage", "VenetianMap":"#VenetianFloorMapImage", "CityViewMap":"#CityViewImage"}; 
    // the offset is defined in the class of 'this' area...it's ghetto but it works
    var offset = $(this).attr('class').replace('area ', ''); 
    // getting the id of 'this' and looking it up in our map
    var objectToChange = map[$(this).parent().attr('id')];
    // shifting the image sprite by a certain number of pixels
    $(objectToChange).css('margin-top', offset);
   });
   $('#VenetianMap area, #LVCCMap area, #CityViewMap area').live('mousedown', function(event) {
    var existingObjectMap = {"LVCCMap":"LVCC", "VenetianMap":"Venetian", "CityViewMap":"CityView"}; 
    var existingObject = existingObjectMap[$(this).parent().attr('id')];
    var newObjectMap = {"LVCCInsert":"LVCC", "VenetianInsert":"Venetian", "CityViewInsert":"CityView"}; 
    var newObject = newObjectMap[$(this).attr('id')];
    // newObject will only have a value if it's associated with a new map, otherwise the user has clicked on something internal to the map and we're not displaying a new map
    if ( newObject === undefined ) {
     showMapInfo($(this));
    } else {
     $('.' + newObject + 'MapContent').show().children().show();
     $('.' + existingObject + 'MapContent').hide();
     buildGreenHeaderBox('Exhibit Map 2012', newObject);
     $('.' + existingObject).fadeOut(700, function () {
      $('.' + newObject).fadeIn(700, function () {
       positionImageMap();
      }); 
     });
    }
    
   });
 
   
        });
        
    </SCRIPT>

