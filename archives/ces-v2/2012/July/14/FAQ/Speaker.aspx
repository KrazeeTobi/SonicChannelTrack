          $(this).parent().children(".faqquestionanswer").each(function(){
              $(this).removeClass("open");
              $(this).find(".faqquestion").removeClass("open");
              $(this).find(".faqanswer").hide();
              $(this).find(".faqanswer").removeClass("open");
          });
         }
         else
         {
          $(this).addClass("open");
          
              $(this).text("Collapse All");
          
          $(this).parent().children(".faqquestionanswer").each(function(){
              $(this).addClass("open");
              $(this).find(".faqquestion").addClass("open");
              $(this).find(".faqanswer").show();
              $(this).find(".faqanswer").addClass("open");
          });
         }
       
    });
    
    $(".faqquestion").click(function() {
    
          $(this).parent().toggleClass("open");
          $(this).toggleClass("open");
          $(this).next().toggle();
          $(this).next().toggleClass("open");
       
    });
    
    
        $("#filter").keyup(function(){
	    
	    $('.faqquestionanswer').hide();
	    $('.faqquestionanswer').removeClass("open");
	    
	    
        // Retrieve the input field text and reset the count to zero
        var filter = $(this).val(), count = 0;
 
        // Loop through the comment list
        
        if(filter.length != 0)
        {
        
	        $(".faqquestionanswer .faqquestion").each(function(){
	 
	            // If the list item does not contain the text phrase fade it out
	            if ($(this).text().search(new RegExp(filter, "i")) < 0 && $(this).next().text().search(new RegExp(filter, "i")) < 0) {
	                $(this).parent().hide();
	                $(this).parent().removeClass("open");
	 
	            // Show the list item if the phrase matches and increase the count by 1
	            } else {
	            	$(this).parent().parent().show();
	            	
	            	$(this).parent().show();
	            	$(this).parent().addClass("open");
	                $(this).show();
	                $(this).addClass("open");
	                $(this).next().show();
	                $(this).addClass("open");
	                count++;
	            }
	        });
        }
        else{
	        $('.faqquestionanswer').show();
	        $('.faqquestionanswer').removeClass("open");
	        $('.faqcategoryheader').removeClass("open");
	        $('.faqanswer').hide();
	        $('.faqanswer').removeClass("open");
	        
	        $('.expand').text("Expand All");
	        $('.expand').removeClass("open");
	        
        }
        // Update the count
        var numberItems = count;
        //$("#filter-count").text("Number of Comments = "+count);
    });
    
    
    
});
