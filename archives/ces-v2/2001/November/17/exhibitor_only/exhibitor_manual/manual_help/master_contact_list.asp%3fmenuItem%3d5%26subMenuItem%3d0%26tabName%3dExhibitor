<script language="JavaScript">
function headerLocalNavConstant() 
	{
	var pathArray = thePath.split("/");
	var headerNavItem;
	if (pathArray[4] == "master_contact_list.asp") 
	{
		headerLocalNavItem = "contacts";
	} 
	else if (pathArray[4] == "faqs.asp")
	{
		headerLocalNavItem = "faqs";
	} 
	else if (pathArray[4] == "glossary.asp")
	{
		headerLocalNavItem = "glossary";
	} 
	else if (pathArray[4] == "tips_tricks.asp") 
	{
		headerLocalNavItem = "tips";
	} 
	else {
		headerLocalNavItem = "none";
	}

	//alert(headerLocalNavItem);

	return headerLocalNavItem;
}
</script>
