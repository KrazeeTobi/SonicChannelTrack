<div class="headerbox">
      <h1>Operations</h1> 
</div>
<div id="inner-content-left">
    <div align="center">
 [ <a href="/For-Exhibitors/Exhibitor-Source-Newsletter/December-2012/News.aspx">News</a> ]  [ <a href="/For-Exhibitors/Exhibitor-Source-Newsletter/December-2012/Operations.aspx">Operations</a> ]  [ <a href="/For-Exhibitors/Exhibitor-Source-Newsletter/December-2012/Venetian-Corner.aspx">Venetian Corner</a> ] 
