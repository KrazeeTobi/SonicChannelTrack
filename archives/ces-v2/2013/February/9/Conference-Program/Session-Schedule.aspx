<script id="tmplLocation" type="text/x-jquery-tmpl">
   <div style="text-align:left; border: solid 1px #ccc;padding: 5px; margin-bottom: 15px; clear: both;">
    <div class="sessionLocation">${Location}</div>
    {{tmpl(RoomList) "#tmplRoom"}}
   </div>
</script><script id="tmplRoom" type="text/html">
   ${logger.debugItem($item.data)}
   {{tmpl(Times) "#tmplTimes"}}
   <table class="report" cellpadding="0" cellspacing="0">
    <tbody><tr class="parent"><td><div class="gridCell" style="width: 60px;">${Room}</div></td></td>
     {{tmpl(SessionList) "#tmplSessionGrid"}}
    </tr>
    <tr class="detail"><td class="showDetail" colspan="16"></td></tr></tbody>
   </table>
</script><script id="tmplTimes" type="text/x-jquery-tmpl">
   <div class="times"><div class="grid" style="width: 60px;">&nbsp;
   </div><div class="grid"><span class="leftArrow">&nbsp;</span>${FirstTime}
   </div><div class="grid">${SecondTime}
   </div><div class="grid">${ThirdTime}</div><div class="grid">${FourthTime}
   </div><div class="grid final" style="border-right: solid 1px #ccc;">${FifthTime}<span class="rightArrow">&nbsp;</span>
   </div><div style="clear: both; font-size: 1px; line-height: 0px;"> &nbsp;</div></div>
</script><script id="tmplSessionGrid" type="text/x-jquery-tmpl">
   ${logger.debugItem($item.data)}
   {{if SessionID == 0}}
    <td style="width: ${SessionDuration}px;"><div style="width: ${SessionDuration}px; background-color: #333;" class="gridCell offset">&nbsp;</div></td>
   {{else}}
    <td style="width: ${SessionDuration}px;" id="td${SessionID}"><div style="width: ${SessionWidth}px;" id="div${SessionID}" class="gridCell offset hover" title="${SessionTitle}"><div style="height: 48px; overflow: hidden; padding-bottom: 5px;"><span class="hoverTitle">${SessionTitle}</span></div>${SessionTime}</div></td>
   {{/if}}
</script><script id="tmplCart" type="text/x-jquery-tmpl">
   <div style="float:right; margin-top:10px; padding-bottom: 10px; padding-left: 10px;">
    {{if formatHelpers.itemInCart(Code)==1}}
     <img style="margin-top: -10px;" src="/cesweb/media/CESWeb/assets/js/ConferenceProgram/images/checkmark.jpg" width="26" alt="added to cart" /><br />
     <a  href="cart.htm" alt="view cart">view cart</a>
    {{else formatHelpers.itemInPurchaseOptions(Code).Type==1}}
     <img style="margin-top: -10px;"  src="/cesweb/media/CESWeb/assets/js/ConferenceProgram/images/valueAdd.jpg" alt="Value Added Upsell" /><br />
     Upsell triggered<a  href="">What does this mean?</a><br />
     <a  href="cart.htm" alt="view cart">view cart</a>
    {{else formatHelpers.itemInPurchaseOptions(Code).Type==2}}
     Included In - <b>{{html formatHelpers.codeLookup(formatHelpers.itemInPurchaseOptions(Code).PurchasedObject)}}</b><br />
     <a  href="cart.htm" alt="view cart">view cart</a>
    {{else}}
     <div id="addToCart${Code}" class="addToCart blueButton">Add to Cart - {{html formatHelpers.getPrice($item.data)}}</div>
    {{/if}}
   </div>
</script><script id="tmplPricing" type="text/x-jquery-tmpl">
   ${Description} - &#36;${Price}<br />
</script><script id="tmplSessionDate" type="text/x-jquery-tmpl">
   <div class="searchResultsDateList" id="${DateID}">${SessionDate}</div>
   {{tmpl(Sessions) "#tmplSearchResults"}}
</script><script id="tmplSearchResults" type="text/x-jquery-tmpl">
   <div style="font-family: Arial; font-size:12px; line-height: 17px; text-align: center;" class="sessionListItem" id="${SessionID}">
    <div class="main listTime">${StartTime} - ${EndTime}</div>
    <div class="sidebar title">${Title}</div>
    <div style="clear:both;"></div>
   </div>
</script><script id="tmplSessionList" type="text/x-jquery-tmpl">
            <li>${Title}</li>
</script><script id="tmplSessionInformation" type="text/x-jquery-tmpl">
   <div class="expand borderStuff" id="SessionDetail${ID}">
    <div class="header">
     <table><tr><td style="width: 160px">&nbsp;</td>
     <td style="width: 346px"><div class="" style="text-align: center!important">
      {{if Logo.length > 0}}
       <img alt="track" src="${Logo}" />
      {{/if}}
     </div></td>
     <td style="width: 160px" valign="top">
     <div style="float: right;">
      {{if $(formatHelpers.getSessionPricing(Code)).length > 0}}
       {{tmpl($item.data) "#tmplCart"}}
      {{/if}}
     </div></td>
     </tr></table>
     <div class="sessionDate center" style="clear: both;">
      <h3 style="text-align: center;">${Title}</h3>
      {{html $item.formatDate(SessionStartTime, SessionEndTime)}}<br />
      ${Location}&nbsp;${Room}<br />
     </div>
     <div>
     <div style="float: right;">{{html formatHelpers.displaySponsorLogos(SponsorLogos)}}</div>{{html Description}}
     </div>
     <hr style="clear: both; width: 90%;" />
    </div>
    <div class="main" style="clear: left; font-weight: normal;">
     {{if $data.Presenters.length > 0}}
      <div class="presenter"><b>Presenter(s):&nbsp;</b><ul class="speaker">{{tmpl($data.Presenters) "#tmplSessionSpeakers"}}</ul><br /></div>
     {{/if}}
     {{if $data.Moderators.length > 0}}
      <div class="moderator"><b>Moderator(s):&nbsp;</b><ul class="speaker">{{tmpl($data.Moderators) "#tmplSessionSpeakers"}}</ul><br /></div>
     {{/if}}
     {{if $data.Panelists.length > 0}}
      <div class="panelist"><b>Panelist(s):&nbsp;</b><ul class="speaker">{{tmpl($data.Panelists) "#tmplSessionSpeakers"}}</ul></div>
     {{/if}}
     {{if $data.Keynotes.length > 0}}
      <div class="keynote"><b>Keynote:&nbsp;</b><ul class="speaker">{{tmpl($data.Keynotes) "#tmplSessionSpeakers"}}</ul></div>
     {{/if}}
     {{if $data.Instructors.length > 0}}
      <div class="instructor"><b>Instructor(s):&nbsp;</b><ul class="speaker">{{tmpl($data.Instructors) "#tmplSessionSpeakers"}}</ul></div>
     {{/if}}
    </div>
    <div>
    <a style="clear: both; float:right;"  href="#" class="hideSessionBody">hide</a>
    <div style="clear:both;"></div>
    </div>
   </div>
</script><script id="tmplSessionSpeakers" type="text/x-jquery-tmpl">
   <li>
    ${FirstName}&nbsp;${LastName}&nbsp;&ndash;&nbsp;${Title},&nbsp;<span style="color: #555; font-weight: bold;">${Company}</span>
  </li>
</script>
