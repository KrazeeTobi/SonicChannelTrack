    var activeNavId=0;
    var thisPageUrl='/attendees/conferences/search/session_detail.asp';
    
        var parentLinks=new Array('dummy','/attendees/default.htm','/attendees/conferences/default.asp','/attendees/conferences/search_by_name.asp');
    
        function checkActiveNav(theUrl) {
            var linkUrl=theUrl;
            var returnValue=false;
            
            if (linkUrl==thisPageUrl) {
                returnValue=true;
            } else if (parentLinks.length>2 && linkUrl==parentLinks[parentLinks.length-1]) {
                returnValue=true;   
            } else if (parentLinks.length>3 && linkUrl==parentLinks[parentLinks.length-2]) {
                returnValue=true;
            } else if (parentLinks.length>4 && linkUrl==parentLinks[parentLinks.length-3]) {
                returnValue=true;
            }
        return returnValue;
    }
