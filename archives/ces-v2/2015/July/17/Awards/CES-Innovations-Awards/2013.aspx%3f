        var hashElem = HashElem_p_lt_ctl08_pageplaceholder_p_lt_ctl09_BizForm1_viewBiz_Country_uniSelectorCountry();
        if (hashElem != null) {
            hashElem.value = selector.options[selector.selectedIndex].getAttribute('data-hash');
        }
    }
}
