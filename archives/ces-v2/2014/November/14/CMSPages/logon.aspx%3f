<script type="text/javascript">
	//<![CDATA[


function titleFullScreen(sender)
{
    if(top.toggleFullScreen)
    {
        top.toggleFullScreen();
        toggleFullScreen(sender);
    }
    return false;
}

function toggleFullScreen(sender)
{
    var btn = sender;
    btn.toggleClass('FullScreenBackButton');
    if(btn.hasClass('FullScreenBackButton'))
    {
        sender.attr('title', 'Restore');
    }
    else
    {
        sender.attr('title', 'Full screen');
    }
}

//]]>
</script>
<script type="text/javascript">
	//<![CDATA[




function fs_reqScript_ScriptTitle(sender)
{
    return titleFullScreen(sender);
}
//]]>
</script>
<script type="text/javascript">
	//<![CDATA[

$j(document).ready(function(){
    if(window.wopener && top.visiblePopup)
    {
        var topFrame = GetTop();
        if(top.isTitleWindow(topFrame, window) && !topFrame.fullScreenButtonAvailable)
        {
            var fsButton = $j('#reqScript_ScriptTitle_pnlFullScreen');
            if(top.isFullScreen())
            {
                toggleFullScreen(fsButton);
            }
            fsButton.show(); 
            $j('#reqScript_ScriptTitle_pnlBody').dblclick(function(){return fs_reqScript_ScriptTitle(fsButton);});
            topFrame.setToFullScreen = function () { return !top.isFullScreen() ? fs_reqScript_ScriptTitle(fsButton) : false; };
            topFrame.fullScreenButtonAvailable = true;
            $j(window).unload(function() {
                topFrame.fullScreenButtonAvailable = false;
            });
        }
    }
});

//]]>
</script>
<script type="text/javascript">
	//<![CDATA[

var hLoc = new Array();
hLoc['ucHelp'] = new Array('Login1_ucHelp_lnkHelp', '/CMSHelp/##TOPIC##.htm');

function SetHelpTopic(name, topic) {
    var l = document.getElementById(hLoc[name][0]);
    if (l != null) {
        l.href = hLoc[name][1].replace("##TOPIC##", topic);
    }
}
//]]>
</script>
