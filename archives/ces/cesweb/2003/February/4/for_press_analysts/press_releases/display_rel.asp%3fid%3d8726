<script language="javascript1.2">
	function checkSearch() {
			var searchForm = document.searchCESWeb;
			
			if(searchForm.SEARCH_STRING.value.search(/\S/) == -1) {
				alert("Please enter text to search for.");
				return;
			}
			searchForm.submit();
	}
</script>
