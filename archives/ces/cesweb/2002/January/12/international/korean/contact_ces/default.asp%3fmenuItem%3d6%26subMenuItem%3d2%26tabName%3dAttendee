<script language="javascript1.2">
	function setSubject() {
		var queryStr = location.search.substring(1,location.search.length);
		var emailMenuParams = queryStr.split("&");
		var emailSubjectIndex = new Number(0);
		var emailSubject = new String("");
		var temp;
		var obj = document.emailContactForm.subject;
		
		if(queryStr != ""){
			for(i=0;i<emailMenuParams.length;i++) {
				temp = emailMenuParams[i].split("=");
				if (temp[0] == "emailSubject") {
					emailSubject = temp[1];
				} 
			}
		
			for(var i=0; i<obj.options.length; i++) {
			  if(obj.options[i].value == emailSubject) {
			    obj.selectedIndex = i;
			    break;
			  }
			}
		}	
	}
</script>
