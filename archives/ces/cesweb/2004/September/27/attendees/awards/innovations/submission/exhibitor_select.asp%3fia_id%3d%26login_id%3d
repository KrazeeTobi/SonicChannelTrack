	<script language="javascript">
	function CheckFields(){
		//Exhibitor
		if(document.searchByExhibitorName.exhibitorName.value == ""){ 
			alert("You must enter a value for Exhibitor Name.")
			searchByExhibitorName.exhibitorName.focus()
			return false
		}
	}
	function submit(letter)
		{
			document.searchByExhibitorName.firstLetter.value = letter;
			document.searchByExhibitorName.exhibitorName.value ="";
			document.searchByExhibitorName.submit();
		}

	</script>
