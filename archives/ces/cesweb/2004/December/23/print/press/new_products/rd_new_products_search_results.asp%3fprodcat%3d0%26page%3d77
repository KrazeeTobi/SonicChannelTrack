var parentLinks=new Array('dummy','/print/press/default.htm','/print/press/new_products/default.asp');

function checkActiveNav(theUrl) {
    var linkUrl=theUrl;
    var returnValue=false;
    
    if (linkUrl==thisPageUrl) {
        returnValue=true;
