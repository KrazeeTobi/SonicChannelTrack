<!-- This Script controls the rollover buttons on the search interface

if (document.images) {
SrchButtName_F1 = new Image(48,18); SrchButtName_F1.src = "/Graphics/cf-bin/Search/SrchButtName.gif";
SrchButtName_F2 = new Image(48,18); SrchButtName_F2.src = "/Graphics/cf-bin/Search/SrchButtName_F2.gif";
SrchButtBrand_F1 = new Image(45,18); SrchButtBrand_F1.src = "/Graphics/cf-bin/Search/SrchButtBrand.gif";
SrchButtBrand_F2 = new Image(45,18); SrchButtBrand_F2.src = "/Graphics/cf-bin/Search/SrchButtBrand_F2.gif";
SrchButtCat_F1 = new Image(64,18); SrchButtCat_F1.src = "/Graphics/cf-bin/Search/SrchButtCat.gif";
SrchButtCat_F2 = new Image(64,18); SrchButtCat_F2.src = "/Graphics/cf-bin/Search/SrchButtCat_F2.gif";
SrchButtProd_F1 = new Image(58,18); SrchButtProd_F1.src = "/Graphics/cf-bin/Search/SrchButtProd.gif";
SrchButtProd_F2 = new Image(58,18); SrchButtProd_F2.src = "/Graphics/cf-bin/Search/SrchButtProd_F2.gif";
}

/* Function that swaps images. */

function di20(id, newSrc) {
    var theImage = FWFindImage(document, id, 0);
    if (theImage) {
        theImage.src = newSrc;
    }
}

/* Functions that track and set toggle group button states. */

function FWFindImage(doc, name, j) {
    var theImage = false;
    if (doc.images) {
        theImage = doc.images[name];
    }
    if (theImage) {
        return theImage;
    }
    if (doc.layers) {
        for (j = 0; j < doc.layers.length; j++) {
            theImage = FWFindImage(doc.layers[j].document, name, 0);
            if (theImage) {
                return (theImage);
            }
        }
    }
    return (false);
}




 

/* --> */
