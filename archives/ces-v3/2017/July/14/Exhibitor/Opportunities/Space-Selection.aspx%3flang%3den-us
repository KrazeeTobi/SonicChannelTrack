    jQuery(document).ready(function ($) 
        {
            var a = $('div.lang > li');
		if(a.length<=1)
		{
			$('#languageSelector').parent().hide();
			
		}	
else
{
            for( var i = 0; i < a.length; i+=3 ) {
                a.slice(i, i+3).wrapAll('<ul class="columns"></ul>');
            }
}
    });
 </script>

