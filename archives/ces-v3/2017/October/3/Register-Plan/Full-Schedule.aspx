<script lang="javascript" type="text/javascript">
    jQuery(document).ready(function ($) {
        var __showDays;
        Calendar = {}
        Calendar.showDay = function () {
            var _text = '';
            if ($('#ShowDays').val() && $('#ShowDays').val() != 'Date') {
                _text = $('#ShowDays').val();
            }
            return _text != '' ? '&showDays=' + _text : '';
        };
        Calendar.timeSpan = function () {
            var _text = '';
            if ($('#TimeSpans').val() && $('#TimeSpans').val() != 'Time') {
                _text = $('#TimeSpans').val();
            }
            return _text != '' ? '&timeSpans=' + _text : '';
        };
        Calendar.location = function () {
            var _text = '';
            if ($('#Locations').val() && $('#Locations').val() != 'Location') {
                _text = $('#Locations').val();
            }
            return _text != '' ? '&locations=' + _text : '';
        };
        Calendar.eventType = function () {
            var _text = '';
            if ($('#EventType').val() && $('#EventType').val() != 'Event Type') {
                _text = $('#EventType').val();
            } else if ($('#hiddenEventType').val() != '') {
                _text = $('#hiddenEventType').val();
            }
            return _text != '' ? '&eventType=' + encodeURIComponent(_text) : '';
        };
        Calendar.trackName = function () {
            var _text = '';
            if ($('#Track').val() && $('#Track').val() != 'Track') {
                _text = $('#Track').val();
            } else if ($('#hiddenTrackName').val() != '') {
                _text = $('#hiddenTrackName').val();
            }
            return _text != '' ? '&track=' + _text : '';
        };
        Calendar.passName = function () {
            var _text = '';
            if ($('#Pass').val() && $('#Pass').val() != 'Pass') {
                _text = $('#Pass').val();
            } else if ($('#hiddenPassName').val() != '') {
                _text = $('#hiddenPassName').val();
            }
            return _text != '' ? '&Pass=' + _text : '';
        };
        Calendar.searchText = function () {
            var _text = $('#calendarSearch').val();
            return _text != '' && _text != 'SEARCH' ? '&searchText=' + $('#calendarSearch').attr('value') : '';
        };
        Calendar.pastEvents = function () {
            var _text = 'false';
            if ($('#pastEvents').is(':checked'))
                _text = 'true';
            return '&pastEvents=' + _text;
        };
        Calendar.freeSessions = function () {
            var _text = 'false';
            if ($('#freeSessions').is(':checked'))
                _text = 'true';
            return '&free=' + _text;
        };
        Calendar.totalPages = function () {
            return totalRecords % recordsPerPage == 0 ? parseInt(totalRecords / recordsPerPage) : parseInt(totalRecords / recordsPerPage) + 1;
        }
        Calendar.viewPortX = $(window).width();
        Calendar.viewPortY = $(window).height();
        Calendar.showPagination = Calendar.viewPortX >= 820;
        var totalRecords; var pageNumber = 1; var recordsPerPage = 10;
        //initialLoad();
        getSessions(true);

        $(".itemsPerPage").on("click", function () {
            var _oldElem = '#' + $(this).parent().find('.active').attr('id');
            var _oldText = $(_oldElem).text();
            $(_oldElem).text('');
            $('<a/>', { href: '#', text: _oldText }).appendTo($(_oldElem))
            $('.itemsPerPage').removeClass('active');
            var _newText = $(this).find('a').text();
            $(this).addClass('active');
            //$(this).find('a').remove();
            //$(this).text(_newText);
            recordsPerPage = _newText;
            pageNumber = 1;
            getSessions();
        });

        $("#resetButton").on("click", function () {
            reset();
            getSessions();
            return false;
        });

        $("#searchButton").on("click", function () {
            getSessions();
            return false;
        });

        $("#pastEvents").on("click", function () {
            showHideShowDays();
            getSessions();
        });

        $("#freeSessions").on("click", function () {
            $("#freeSessions").prop("disabled", true);
            getSessions();
        });

        $(".prev, .next").on("click", function (e) {
            if ($(this).hasClass('prev')) pageNumber -= 1;
            if ($(this).hasClass('next')) pageNumber += 1;
            if (pageNumber < 1) pageNumber = 1;
            if (pageNumber > Calendar.totalPages()) pageNumber = Calendar.totalPages();
            getSessions();
            return false;
        });

        $('body').on('click', '.pageNumber', function () {
            pageNumber = parseInt(parseInt($(this).text()));
            getSessions();
            return false;
        });

        function showHideShowDays() {
            _.each($("#ShowDays option"), function (item) {
                if (!($(item).val() == 'Date')) {
                    $(item).remove();
                }
            });
            var _checked = $('#pastEvents').is(':checked');
            $(__showDays).each(function (i, item) {
                var d = new Date();
                d.setHours(0, 0, 0, 0);
                if ((Date.parse(item.Key) >= d && !_checked) || _checked) {
                    $('#ShowDays').append('<option class="calendarFilterOption" value="' + item.Key + '\">' + item.Value + '</option>');
                }
            });
        }

        function reset() {
            $("#ShowDays").val($("#ShowDays option:first").val());
            showHideShowDays();
            $("#TimeSpans").val($("#TimeSpans option:first").val());
            $("#Locations").val($("#Locations option:first").val());
            $("#EventType").val($("#EventType option:first").val());
            $("#Track").val($("#Track option:first").val());
            $('#calendarSearch').val('SEARCH');
            $("#ConferenceSessionFilters").hide();
            $("#freeSessions").prop("disabled", false).prop("checked", false);
        }

        function getSessions(initialLoad) {
            jQuery("#myModal").modal('show');
            initialLoad = initialLoad || false;

            var param = "";
            param += Calendar.showDay();
            param += Calendar.timeSpan();
            param += Calendar.location();
            param += Calendar.eventType();
            if ($('#EventType').val() == 'ConferenceSession') {
                param += Calendar.trackName();
                param += Calendar.passName();
            }
            param += Calendar.searchText();
            param += Calendar.pastEvents();
            param += Calendar.freeSessions();
            if (initialLoad) {
                param += "&initialLoad=true";
            }
            $.when(
                $.ajax('/CEA_CMS_Items/Handlers/CES/getSessions.ashx?Verb=GetSessions&PageSize=' + recordsPerPage + '&page=' + pageNumber + param)
            ).then(function (cal) {
                if (!_.isUndefined(cal)) {
                    //var cal = $.parseJSON(response);
                    totalRecords = cal.Count;
                    if (cal.DisplayTracks || $('#EventType').val() == 'ConferenceSession') {
                        $("#ConferenceSessionFilters").show();
                    } else {
                        $("#ConferenceSessionFilters").hide();
                    }

                    var _templateTable = '{{#Calendar}}<tr><td data-title="Event Type"><div class="event-logo hidden-xs"><img src="{{Logo}}"><br /></div><span class="sessionType">{{SessionType}}</span></td><td data-title="Event"><h3><a href="{{Url}}">{{Title}}</a></h3><span class="hidden-xs">{{{StrippedDescription}}}</span></td><td  data-title="Date">{{FormattedDate}}</td><td data-title="Time">{{{FormattedTime}}}</td><td data-title="Location">{{Building}}{{#Building}}<br/>{{/Building}}{{Room}}{{#Room}}<br/>{{/Room}}{{Location}}</td></tr>{{/Calendar}}';
                    $('#calendarTable > tbody > tr').remove();
                    $('#calendarTable > tbody').append(Mustache.to_html(_templateTable, cal));
                    calculatePages();
                    if (!cal.AnyFree)
                        $("#freeSessions").prop("disabled", true);
                    
                    populateFilters(cal.ShowDays, cal.TimeSpans, cal.Locations, cal.EventType, cal.Tracks, cal.Passes);
                    showHideShowDays();
                    $('#Track').val(cal.SelectedTrack);
                    $('#Pass').val(cal.SelectedPass);
                    $('#EventType').val(cal.SelectedEventType);
                    $('#Locations').val(cal.SelectedLocation);
                    $('#ShowDays').val(cal.SelectedDate);
                    $('#TimeSpans').val(cal.SelectedTimeSpan);
                    //}
                    jQuery("#myModal").modal('hide');
                }
            });
        }

        function calculatePages() {
            if (!Calendar.showPagination) return;
            //var totalPages = totalRecords % recordsPerPage == 0 ? parseInt(totalRecords / recordsPerPage) - 1 : parseInt(totalRecords / recordsPerPage);
            var bEllipse = true;
            //var elem = $('#calendarPageNo');// $('ul.pagination');
            //$(elem).find('li.pageNumber').remove();
            $('#calendarPageNo').find('li.pageNumber').remove();
            for (var i = 1; i <= Calendar.totalPages() ; i++) {
                $('<a/>', {
                    href: '#', text: i
                }).appendTo($('<li/>', { 'id': 'pageNumber' + i, 'class': 'pageNumber' }).insertAfter($('#pageNumber' + parseInt(i - 1))));
            }
            $('#pageNumber' + pageNumber).addClass('active');
            //$('#pageNumber' + pageNumber).find('a').remove();

            var bEllipse = true; var lastKnown = $('#pageNumber1');
            $('li.ellipse').remove();
            for (var i = 1; i <= Calendar.totalPages() ; i++) {
                var li = $('#pageNumber' + i);
                if ((i >= 3 && i < Calendar.totalPages() - 1) && (i < pageNumber - 1 || i > pageNumber + 1)) {
                    $(li).remove();
                    if (bEllipse) {
                        bEllipse = false;
                        $('<a/>', {
                            href: '#', text: '...'
                        }).appendTo($('<li/>', { 'id': 'ellipse', 'class': 'ellipse disabled' }).insertAfter($(lastKnown)));
                    }
                } else if ($(li).length > 0) {
                    bEllipse = true;
                    lastKnown = li;
                }
            }
            // items per page
            _.each($('.itemsPerPage'), function (item) {
                var p = $(item).attr('id').replace('pageSize', '');
                if (parseInt(p) > totalRecords) {
                    $(item).addClass('hide');
                } else {
                    $(item).removeClass('hide');
                }
            });
        }



        function populateFilters(showDays, timeSpans, locations, EventType, tracks, passes) {
            $('.calendarFilter option.calendarFilterOption').remove();
            __showDays = showDays;
            populateSelectFilter('TimeSpans', timeSpans);
            populateSelectFilter('Locations', locations);
            populateSelectFilter('EventType', EventType);
            populateSelectFilter('Track', tracks);
            populateSelectFilter('Pass', passes);
        }

        function populateSelectFilter(selectID, itemsList) {

            $(itemsList).each(function (i, item) {
                $('#' + selectID).append('<option class="calendarFilterOption" value="' + item.Key + '\">' + item.Value + '</option>');
            });

        }

        /////////// Select Bindings ///////////
        $('body').on('change', 'select.calendarFilter', function () {
            if ($('#EventType').val() == 'ConferenceSession') {
                $('#Track').removeClass('hide');
            } else {
                $('#Track').addClass('hide');
            }
            getSessions();
        });

        /////////// Searchbox Bindings ///////////
        //setup before functions
        var typingTimer;
        var doneTypingInterval = 750;

        //on keyup, start the countdown
        $(window).keydown(function (event) {
            if (event.keyCode == 13) {
                getSessions();
                event.preventDefault();
            }
            clearTimeout(typingTimer);
            if ($('#calendarSearch').val)
                typingTimer = setTimeout(doneTyping, doneTypingInterval);
        });

        //$('#calendarSearch').keyup(function (key) {
        //    if (key.which === 13) { getSessions(); }
        //    clearTimeout(typingTimer);
        //    if ($('#calendarSearch').val) {
        //        typingTimer = setTimeout(doneTyping, doneTypingInterval);
        //    }
        //});

        //user is "finished typing," do something
        function doneTyping() {
            var textLen = $('#calendarSearch').attr('value').length;
            if (textLen >= 3 || textLen == 0) {
                getSessions();
            }
        }

        $("#calendarSearch").on("click focusout", function (e) {
            if (e.type == 'click' && $('#calendarSearch').attr('value') == 'SEARCH') $('#calendarSearch').attr('value', '');
            if (e.type == 'focusout' && $('#calendarSearch').attr('value') == '') $('#calendarSearch').attr('value', 'SEARCH');
            if (Calendar.searchText.length > 0) {
                getSessions();
            }
        });

        //            $("#calendarSearch").focusout(function () {
        //                if ($('#calendarSearch').attr('value') == '') $('#calendarSearch').attr('value', 'SEARCH');
        //                getSessions();
        //            });


    });
</script>

