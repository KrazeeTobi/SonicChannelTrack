<script language="javascript" type="text/javascript">
    jQuery(document).ready(function ($) {
        var totalRecords; var pageNumber = 1; var recordsPerPage = 10;
        loadData();

        /////////// Pagination Bindings ///////////
        $(".itemsPerPage").on("click", function () {
            var _oldElem = '#' + $(this).parent().find('.current').attr('id');
            var _oldText = $(_oldElem).text();
            $(_oldElem).text('');
            $('<a/>', { href: '#', text: _oldText }).appendTo($(_oldElem))
            $('.itemsPerPage').removeClass('current');
            var _newText = $(this).find('a').text();
            $(this).addClass('current');
            $(this).find('a').remove();
            $(this).text(_newText);
            recordsPerPage = _newText;
            pageNumber = 1;
            loadData();
        });

        $('body').on('click', '.pageNumber', function () {
            pageNumber = parseInt(parseInt($(this).text()));
            loadData();
            return false;
        });

        $(".prev, .next").on("click", function (e) {
            var totalPages = totalRecords % recordsPerPage == 0 ? parseInt(totalRecords / recordsPerPage) : parseInt(totalRecords / recordsPerPage) + 1;
            if ($(this).hasClass('prev')) pageNumber -= 1;
            if ($(this).hasClass('next')) pageNumber += 1;
            if (pageNumber < 1) pageNumber = 1;
            if (pageNumber > totalPages) pageNumber = totalPages;
            loadData();
            return false;
        });

        function calculatePages() {
            var totalPages = totalRecords % recordsPerPage == 0 ? parseInt(totalRecords / recordsPerPage) : parseInt(totalRecords / recordsPerPage) + 1;
            var bEllipse = true;
            var elem = $('ul.pagination');
            $(elem).find('li.pageNumber').remove();
            for (var i = 1; i <= totalPages; i++) {
                $('<a/>', { href: '#', text: i
                }).appendTo($('<li/>', { id: 'pageNumber' + i, class: 'pageNumber' }).insertAfter($('#pageNumber' + parseInt(i - 1))));
            }
            $('#pageNumber' + pageNumber).addClass('current').text(pageNumber);
            $('#pageNumber' + pageNumber).find('a').remove();

            var bEllipse = true; var lastKnown = $('#pageNumber1');
            $('li.ellipse').remove();
            for (var i = 1; i <= totalPages; i++) {
                var li = $('#pageNumber' + i);
                if ((i >= 4 && i < totalPages - 2) && (i < pageNumber - 1 || i > pageNumber + 1)) {
                    $(li).remove();
                    if (bEllipse) {
                        bEllipse = false;
                        $('<li/>', { class: 'ellipse', text: '...' }).insertAfter($(lastKnown));
                    }
                } else if ($(li).length > 0) {
                    bEllipse = true;
                    lastKnown = li;
                }
            }
            // items per page
            _.each($('.itemsPerPage'), function (item) {
                var p = $(item).attr('id').replace('pageSize', '');
                if (parseInt(p) > totalRecords) {
                    $(item).addClass('hide');
                } else {
                    $(item).removeClass('hide');
                }
            });
        }

        /////////// Searchbox Bindings ///////////
        //setup before functions
        var typingTimer;
        var doneTypingInterval = 750;

        //on keyup, start the countdown
        $('#nameSearch, #companySearch, #keywordSearch').keyup(function (key) {
            if (key.which === 13) { loadData(); }
            clearTimeout(typingTimer);
            typingTimer = setTimeout(doneTyping, doneTypingInterval);
        });

        //user is "finished typing," do something
        function doneTyping() {
            var _nameSearch = $('#nameSearch').attr('value');
            var _companySearch = $('#companySearch').attr('value');
            var _keywordSearch = $('#keywordSearch').attr('value');
            if (_nameSearch != 'Name' || (!_companySearch != 'Company' && _companySearch.length >= 3) || (_keywordSearch != 'Keyword' && _keywordSearch.length >= 3)) {
                loadData();
            }
        }

        $('.searchButton').on('click', function (e) {
            loadData();
            return false;
        });

        $('#displayAll').on('click', function (e) {
            pageNumber = 1;
            $('#keywordSearch').attr('value', 'Keyword');
            $('#nameSearch').attr('value', 'Name');
            $('#companySearch').attr('value', 'Company');
            loadData();
            return false;
        });

        $("#nameSearch, #companySearch, #keywordSearch").on("click focusout", function (e) {
            if ($(this).attr('id') == 'nameSearch') {
                if (e.type == 'click' && $('#nameSearch').attr('value') == 'Name') $('#nameSearch').attr('value', '');
                if (e.type == 'focusout' && $('#nameSearch').attr('value') == '') {
                    $('#nameSearch').attr('value', 'Name');
                    loadData();
                }
                $('#companySearch').attr('value', 'Company');
                $('#keywordSearch').attr('value', 'Keyword');
            } else if ($(this).attr('id') == 'companySearch') {
                if (e.type == 'click' && $('#companySearch').attr('value') == 'Company') $('#companySearch').attr('value', '');
                if (e.type == 'focusout' && $('#companySearch').attr('value') == '') {
                    $('#companySearch').attr('value', 'Company');
                    loadData();
                }
                $('#nameSearch').attr('value', 'Name');
                $('#keywordSearch').attr('value', 'Keyword');
            } else if ($(this).attr('id') == 'keywordSearch') {
                if (e.type == 'click' && $('#keywordSearch').attr('value') == 'Keyword') $('#keywordSearch').attr('value', '');
                if (e.type == 'focusout' && $('#keywordSearch').attr('value') == '') {
                    $('#keywordSearch').attr('value', 'Keyword');
                    loadData();
                }
                $('#nameSearch').attr('value', 'Name');
                $('#companySearch').attr('value', 'Company');
            }
        });

        function loadData() {
            var _nameSearch = $('#nameSearch').val() == 'Name' ? '' : $('#nameSearch').val();
            var _companySearch = $('#companySearch').val() == 'Company' ? '' : $('#companySearch').val();
            var _keywordSearch = $('#keywordSearch').val() == 'Keyword' ? '' : $('#keywordSearch').val();
            $.when(
		        $.getJSON('/CEA_CMS_Items/Handlers/getTransformationTemplate.ashx?callback=?', { templateName: "session.speaker.mustacheSpeakerSearch" }),
		        $.getJSON('/CEA_CMS_Items/Handlers/CES/GetSpeakerSearchData.ashx', { name: _nameSearch, company: _companySearch, keyword: _keywordSearch, pageSize: recordsPerPage, page: pageNumber })
            ).then(function (template, data) {
                var _template = _.first(template);
                var _parsedObject = JSON.parse(_.first(data));
                totalRecords = _parsedObject.Count;
                //$('#speakerListingContainer').html('')
                $('#speakerListingContainer').html(Mustache.to_html(_template, _parsedObject));
                calculatePages();
            });

        }

    });
</script>
