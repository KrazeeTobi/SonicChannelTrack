<script>
    $(document).ready(function () {
        $(".bg-image-info").each(function () { 
            var $heroBgImgClass = $(this).children("#bgImgClass").attr("value");
            var $bgImgSmClass = $(this).children("#bgImgSmClass").attr("value");
            var $heroBgImg = $(this).children("#backgroundImg").attr("value");
            var $heroBgImgSmall = $(this).children("#bgImgSmall").attr("value");
            var $classHtmlElement = document.getElementsByClassName($heroBgImgClass);
            var $classHtmlElementSmall = document.getElementsByClassName($bgImgSmClass);
            
            if ($heroBgImgSmall === undefined) {
                $($classHtmlElement).css({ "background-image": "url(" + $heroBgImg + ")" });
            } else {
                $($classHtmlElement).css({ "background-image": "url(" + $heroBgImg + ")" });
                $($classHtmlElementSmall).css({ "background-image": "url(" + $heroBgImgSmall + ")" });
            }
        });
    });
</script>
        <div class="container">
            <div class="row">
                <div class="col-12 col-md-6 col-lg-5 ml-md-auto topic-hero__content-col">
