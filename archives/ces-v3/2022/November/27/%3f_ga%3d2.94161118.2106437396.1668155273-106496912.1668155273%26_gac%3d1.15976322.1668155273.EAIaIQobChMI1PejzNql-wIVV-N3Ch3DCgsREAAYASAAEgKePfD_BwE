  function EnterEventMobile(e) {
      if (e.keyCode == 13) {
          var val = document.getElementById('nav-mobile-search-input').value;
          var sURL = '/search-results?searchtext=' + val + '&searchmode=anyword';
          window.location.href = sURL;
          return false;
      }
  }

