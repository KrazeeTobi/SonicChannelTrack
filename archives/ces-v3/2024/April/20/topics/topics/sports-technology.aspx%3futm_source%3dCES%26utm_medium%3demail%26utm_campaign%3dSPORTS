    <input type="hidden" name="p$lt$WebPartZone9$zoneContent$pageplaceholder$p$lt$WebPartZone1$HeaderZone$CESEditableBackgroundImage$bgImgSmall" id="bgImgSmall" value="https://cdn.ces.tech/ces/media/events-experiences-images/2024/topicpage_digitalhealth_800x1330.png?ext=.png" />
