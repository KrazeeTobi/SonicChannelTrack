  </div>
  <script>
    jQuery(document).ready(function($) {
      if($('.nav-desktop__panel .col:last-child').hasClass('featured')) {} 
      else {
        $('.nav-desktop__panel').removeClass('nav-desktop__panel--featured');
      }
    });
  </script>
</li>
