    <header class="global-hero text-block-font-styles">
    <section class="container">
        <div class="row">
            <div class="col-12">
                <div class="row">
                    <div class="col-12 col-lg-9">
                        <h1 class="global-hero__title">CES App Support</h1>
                        
                    </div>
                </div>
                <hr class="global-hero__rule">
