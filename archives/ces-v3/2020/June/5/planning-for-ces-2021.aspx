    <header class="global-hero text-block-font-styles">
    <section class="container">
        <div class="row">
            <div class="col-12">
                <div class="row">
                    <div class="col-12 col-lg-9">
                        <h1 class="global-hero__title">Planning for CES 2021</h1>
                        <p class="global-hero__description"><strong><small><em>Last updated May 7, 2020</em></small></strong></p>
                    </div>
                </div>
                <hr class="global-hero__rule">
            </div>
        </div>
