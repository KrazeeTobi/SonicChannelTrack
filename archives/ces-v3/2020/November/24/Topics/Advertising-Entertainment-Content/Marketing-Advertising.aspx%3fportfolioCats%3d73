<div class="bg-image-info">
    <input type="hidden" name="p$lt$WebPartZone9$zoneContent$pageplaceholder$p$lt$WebPartZone1$HeaderZone$CESEditableBackgroundImage$backgroundImg" id="backgroundImg" value="https://cdn.ces.tech/ces/media/topics/mar-ad/cspace_topic1903x950.jpg?ext=.jpg" />
    <input type="hidden" name="p$lt$WebPartZone9$zoneContent$pageplaceholder$p$lt$WebPartZone1$HeaderZone$CESEditableBackgroundImage$bgImgSmall" id="bgImgSmall" />
    <input type="hidden" name="p$lt$WebPartZone9$zoneContent$pageplaceholder$p$lt$WebPartZone1$HeaderZone$CESEditableBackgroundImage$bgImgClass" id="bgImgClass" value="topic-hero__image" />
    <input type="hidden" name="p$lt$WebPartZone9$zoneContent$pageplaceholder$p$lt$WebPartZone1$HeaderZone$CESEditableBackgroundImage$bgImgSmClass" id="bgImgSmClass" value="topic-hero__image-small" />
</div>
<script>
    $(document).ready(function () {
        $(".bg-image-info").each(function () { 
            var $heroBgImgClass = $(this).children("#bgImgClass").attr("value");
            var $bgImgSmClass = $(this).children("#bgImgSmClass").attr("value");
            var $heroBgImg = $(this).children("#backgroundImg").attr("value");
            var $heroBgImgSmall = $(this).children("#bgImgSmall").attr("value");
            var $classHtmlElement = document.getElementsByClassName($heroBgImgClass);
            var $classHtmlElementSmall = document.getElementsByClassName($bgImgSmClass);
            
            if ($heroBgImgSmall === undefined) {
                $($classHtmlElement).css({ "background-image": "url(" + $heroBgImg + ")" });
            } else {
                $($classHtmlElement).css({ "background-image": "url(" + $heroBgImg + ")" });
                $($classHtmlElementSmall).css({ "background-image": "url(" + $heroBgImgSmall + ")" });
            }
        });
    });
</script>
        <div class="container">
            <div class="row">
                <div class="col-12 col-md-6 col-lg-5 ml-md-auto topic-hero__content-col">
                    <div class="topic-hero__content">
                        <h1 class="topic-hero__title">C SPACE: Marketing & Advertising 



