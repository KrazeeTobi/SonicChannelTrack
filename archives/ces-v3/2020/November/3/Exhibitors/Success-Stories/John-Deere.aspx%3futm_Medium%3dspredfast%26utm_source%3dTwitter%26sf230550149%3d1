  function EnterEvent(e) {
      if (e.keyCode == 13) {
          var val = document.getElementById('nav-desktop-search-field').value;
          var sURL = '/search-results?searchtext=' + val + '&searchmode=anyword';
          window.location.href = sURL;
          return false;
      }
  }
