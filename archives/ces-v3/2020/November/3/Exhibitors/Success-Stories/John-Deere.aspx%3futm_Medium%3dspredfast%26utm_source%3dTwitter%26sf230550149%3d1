  <header class="case-study-hero">
    <div class="case-study-hero__img case-study-hero__img-sm d-md-none"></div>
<div class="case-study-hero__img case-study-hero__img-lg d-none d-md-block"></div>

<div class="bg-image-info">
    <input type="hidden" name="p$lt$WebPartZone9$zoneContent$pageplaceholder$p$lt$WebPartZone1$BackgroundImageZone$CaseStudyEditableBackgroundImage$backgroundImg" id="backgroundImg" value="https://cdn.ces.tech/ces/media/exhibitors/case-studies/pano_right.jpg?ext=.jpg" />
    <input type="hidden" name="p$lt$WebPartZone9$zoneContent$pageplaceholder$p$lt$WebPartZone1$BackgroundImageZone$CaseStudyEditableBackgroundImage$bgImgSmall" id="bgImgSmall" />
    <input type="hidden" name="p$lt$WebPartZone9$zoneContent$pageplaceholder$p$lt$WebPartZone1$BackgroundImageZone$CaseStudyEditableBackgroundImage$bgImgClass" id="bgImgClass" value="case-study-hero__img case-study-hero__img-lg d-none d-md-block" />
    <input type="hidden" name="p$lt$WebPartZone9$zoneContent$pageplaceholder$p$lt$WebPartZone1$BackgroundImageZone$CaseStudyEditableBackgroundImage$bgImgSmClass" id="bgImgSmClass" value="case-study-hero__img case-study-hero__img-sm d-md-none" />
