    <input type="hidden" name="p$lt$WebPartZone8$zoneContent$pageplaceholder$p$lt$WebPartZone1$HeaderZone$CESEditableBackgroundImage$backgroundImg" id="backgroundImg" value="https://cdn.ces.tech/ces/media/topics/vehicle-tech/topic-hero-vehicle-technology_2880.jpg?ext=.jpg" />
    <input type="hidden" name="p$lt$WebPartZone8$zoneContent$pageplaceholder$p$lt$WebPartZone1$HeaderZone$CESEditableBackgroundImage$bgImgSmall" id="bgImgSmall" />
    <input type="hidden" name="p$lt$WebPartZone8$zoneContent$pageplaceholder$p$lt$WebPartZone1$HeaderZone$CESEditableBackgroundImage$bgImgClass" id="bgImgClass" value="topic-hero__image" />
    <input type="hidden" name="p$lt$WebPartZone8$zoneContent$pageplaceholder$p$lt$WebPartZone1$HeaderZone$CESEditableBackgroundImage$bgImgSmClass" id="bgImgSmClass" value="topic-hero__image-small" />
